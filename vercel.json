{
  "version": 2,
  "functions": {
    "api/bookings.js": {
      "maxDuration": 30
    },
    "api/imagekit-portfolio.js": {
      "maxDuration": 30
    },
    "api/portfolio.js": {
      "maxDuration": 10
    },
    "api/imagekit-webhook.js": {
      "maxDuration": 30
    },
    "api/sync-portfolio.js": {
      "maxDuration": 60
    },
    "api/setup-database.js": {
      "maxDuration": 60
    },
    "api/contact.js": {
      "maxDuration": 30
    },
    "api/setup-contact-table.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [{
    "path": "/api/sync-portfolio?action=sync",
    "schedule": "0 5 * * *"
  }]
} 